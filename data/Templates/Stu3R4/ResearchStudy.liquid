{% diff msg -%}
{
  "relatedArtifact" : [ {{ msg.relatedArtifact | to_array | batch_render: 'Shared/RelatedArtifact', 'msg' }} ],
  "extension" : [
    {% for j in msg.jurisdiction -%}
    {
      "url" : "http://hl7.org/fhir/r3/StructureDefinition/extension-ResearchStudy.jurisdiction",
      "valueCodeableConcept" : {{msg.j | to_json_string }}
    },
    {% endfor -%}
  ],
  "arm" : [ {{ msg.arm | to_array | batch_render: 'ResearchStudy/Arm', 'msg' }} ],
  "jurisdiction" : ""
}
{% enddiff -%}