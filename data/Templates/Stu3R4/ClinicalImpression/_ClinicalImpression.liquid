{% mergeDiff msg -%}
{
  "contained" : [ {{msg.contained | to_array | batch_render : 'Resource', 'msg'}} ],
    "encounter" : {{msg.context | to_json_string}},
    "context" : "",
    "extension":[
        {% if msg.action -%}
        {
            "url" : "http://hl7.org/fhir/3.0/StructureDefinition/extension-ClinicalImpression.action",
            "valueReference" : {{msg.action | to_json_string }}
        }
        {% endif -%}
    ],
    "action" : ""
}
{% endmergeDiff -%}
